package ex01;

public class CarTest {

	public static void main(String[] args) {
		Car c1 = new Car();
		Car c2 = new Car("blue");
		c1.color = "white";
		c1.gear = "auto";
		c1.door = 4;
	
		Car c2 = new Car("white","auto",4);
		System.out.println(c1.color,c1.gear,c1.door);
		System.out.println(c2.color,c2.gear,c2.door);

	}

}
