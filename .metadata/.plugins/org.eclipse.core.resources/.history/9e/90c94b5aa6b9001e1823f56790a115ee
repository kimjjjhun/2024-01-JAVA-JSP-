package ex04;

import java.util.Scanner;

public class 연습장 {

	public static void main(String[] args) {
		
		//3개의 문자열을 선언하고 입력받아 차례대로 출력해보자
	
		String[] name = new String[3];
		Scanner sc = new Scanner(System.in);
		System.out.println("세 개의 문자를 입력하세요.");
		for (int a = 0; a<=3; a++) {
			name[a] = sc.nextLine();
		}
	    
	
		for (int a = 0; a<=3; a++) {
			System.out.println(name[a]+"");
		}
	
	
	
	
	
	
	}
	
}
	


