package ex04;

import java.util.Scanner;

public class 연습장 {

	public static void main(String[] args) {
		
		//3개의 문자열을 선언하고 입력받아 차례대로 출력해보자
	
		//int형 배열 생성후 랜덤(1~100)이용해서 배열 크기 10값 저장
				//그 배열 총점.평균 구하기
	double avg = 0;
	int total = 0;
	int[] num = new int[10];
	for(int i=0; i<num.length; i++) {
		int j = (int)(Math.random()*100)+1;
		num[i] += total;
	}
		for (int i=0; i<num.length; i++) {
			System.out.println(num[i] + " ");
	}
	
	}
	
}
	


