package ex02;

public class MP3Player implements MediaPlayer {

	@Override
	public void play() {
		System.out.println("음악재생");
			
		}
	

	@Override
	public void pause() {
		System.out.println("일시정지");
	}

	@Override
	public void stop() {
		System.out.println("되돌리기");
	}

	@Override
	public void file() {
		System.out.println();
	}

	@Override
	public void playOn() {
		
		System.out.println("재생");
	}

	@Override
	public void playOff() {
		System.out.println();
	}

	@Override
	public void playstop() {
		System.out.println();
	}
	
	

	
	

	

}
