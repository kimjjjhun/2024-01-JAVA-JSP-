package ex03;

import java.util.Scanner;

public class ex10 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String b = sc.nextLine();
		System.out.println("금액을 입력하세요." + b);
		int a = sc.nextInt();
		System.out.println("본인의 예금:" + a);
		System.out.println("본인의 출금:" + a);
		System.out.println("본인의 잔고:" + a);
		
		
		
		

	}

}
