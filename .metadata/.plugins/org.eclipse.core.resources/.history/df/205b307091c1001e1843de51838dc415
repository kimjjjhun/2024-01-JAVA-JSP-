package ex01;

public class Car {
	String color;           
	String gear;
	int door;
	
	 Car() {
		 this("white","auto",4);
		
	}
	 Car(String color){
		 this(color,"auto",4);
		 
	 }
     Car(String color,String gear,int door) {
    	this.color = color;
    	this.gear = gear;
    	this.door = door;
    	
    }
}
