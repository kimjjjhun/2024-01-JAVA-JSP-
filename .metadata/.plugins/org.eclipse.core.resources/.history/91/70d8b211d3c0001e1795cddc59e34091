package ex02;

public interface MediaPlayer {
	
	int Max = 1;
	int Min = 0;
	
	default public void play() {
		if(Max<Min){
			System.out.println("재생중");
		}else {
				System.out.println("일시정지");

			}
			
		}
	
	default public void stop() {
		
	}
	default public void off() {
		
	}
     default public void file() {
    	 
     }
     void playOn();
     void playOff();
     void playstop();
     
    
}
