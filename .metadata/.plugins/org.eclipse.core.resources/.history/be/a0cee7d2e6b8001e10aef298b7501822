package ex03;

import java.util.Scanner;

public class For01 {

	public static void main(String[] args) {
        
	       int even = 0;
	       int ood = 0;
	       for(int i=1; i <= 100; i++) {
	    	   if (i % 3 == 1)
	    		   even += i;
	    	   else
	    		   ood += i;
	       }
			
				
			
			
		
			
	
		
		
		
		}
		
		
	}

