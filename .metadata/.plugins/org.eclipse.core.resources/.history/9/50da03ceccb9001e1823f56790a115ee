package ex04;

import java.util.Scanner;

public class 연습장 {

	public static void main(String[] args) {
		
		
	
		//int형 배열 생성후 랜덤(1~100)이용해서 배열 크기 10값 저장
				//그 배열 총점.평균 구하기
		// 로또가 1~45 숫자 중에서 랜덤하게 6개 추출
	
		int lotto = 0;      //초기값 설정
 		boolean flag = false; // 초기값 설정
 		int[] number = new int[6]; // 인트변수값 배열
 		for(int i=0; i<number.length; i++) {  // 반복문실행 - 배열값보다 작을때까지
 			flag = false;     // 불리언 초기값
 			int num = (int)(Math.random()*45)+1;  //45번까지 랜덤번호생성
 			
 		}
		
		
	}
	
}
	


