package ex03;

import java.util.Scanner;

public class For01 {

	public static void main(String[] args) {
        
	       int c3 = 1;
	       for(int i=1; i <= 100; i++) {
	    	   if (i % 3 == 0)  
	    	   System.out.println(+i);
	       }
	    	   
	       int c5 = 1;
	       for(int o=1; o <= 100; o++ ) {
	    	   if (o % 5 == 0)
	    		   System.out.println(+o);
	    	   
	    	   System.out.println(c3 + c5 );
	       }
			
				
			
			
		
			
	
		
		
		
		}
		
		
	}

